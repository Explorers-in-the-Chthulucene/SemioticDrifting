###  http://www.semanticweb.org/ghiot/ontologies/2023/5/chthulucene#Perception_active
:Perception_active rdf:type owl:Class ;
                   rdfs:subClassOf :Frame ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :involvesRole ;
                                     owl:someValuesFrom :Location
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :involvesRole ;
                                     owl:someValuesFrom :Perceiver_agentive
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :involvesRole ;
                                     owl:someValuesFrom :Phenomenon
                                   ] ;
                   rdfs:comment """This frame contains perception words whose perceivers
  intentionally direct their attention to some entity or phenomenon   
  in order to have a perceptual experience.  For this reason we       
  call the perceiver role in this frame Perceiver_agentive.""" .


