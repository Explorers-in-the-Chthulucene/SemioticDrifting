chco:Concept a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom chco:ModeConcept ;
            owl:onProperty chco:hasCondition ],
        [ a owl:Restriction ;
            owl:allValuesFrom chco:ActivityConcept ;
            owl:onProperty chco:impliesActivity ] .

